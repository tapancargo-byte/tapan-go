{
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "target": "ES2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "https://deno.land/std@0.168.0/http/server.ts": ["./_types/deno-http-server"],
      "https://esm.sh/@supabase/supabase-js@2": ["./_types/supabase-js-remote"],
      "https://esm.sh/ai": ["./_types/ai-remote"],
      "https://esm.sh/@ai-sdk/openai": ["./_types/openai-remote"],
      "npm:resend": ["./_types/resend-remote"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "_types/**/*.d.ts"],
  "exclude": ["node_modules"]
}
